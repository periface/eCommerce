@model IEnumerable<PymeTamFinal.Modelos.ModelosDominio.CostosEnvio>
@{ Layout = null;}
@Html.Hidden("idEstado", (int)ViewBag.id)
<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="exampleModalLabel">Agregar metodo de envio</h4>
        </div>
        <div class="modal-body">

            <div class="row">
                @foreach (var metodo in Model)
            {
                    <div class="col-lg-6 form-group">
                        <div class="checkbox">
                            <label style=" white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;">
                                @Html.envioEstadoChk(metodo.idEnvio, (int)ViewBag.id) @metodo.nombreMetodoEnvio

                            </label>
                        </div>
                        <p class="help-block" style=" white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;">
                            @metodo.detalle
                        </p>
                    </div>
                }

            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
        </div>
    </div>
</div>
