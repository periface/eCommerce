@model IEnumerable<PymeTamFinal.Modelos.ModelosDominio.CostosEnvio>
@{ Layout = null;}
<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="exampleModalLabel">Agregar metodo de envio</h4>
        </div>
        <div class="modal-body">
            @foreach (var metodo in Model)
            {
                <div class="row">
                    <div class="col-lg-12 form-group">
                        <a href="#" data-envio="@metodo.idEnvio" data-estado="@ViewBag.id" class="btn btn-primary agregaEnvioAEstado">@metodo.nombreMetodoEnvio</a> <a href="#" data-envio="@metodo.idEnvio" data-estado="@ViewBag.id" class="btn btn-warning remueveEnvioAEstado">Remover</a>
                        <p class="help-block">
                            @metodo.detalle
                        </p>
                    </div>
                </div>
            }
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
        </div>
    </div>
</div>

